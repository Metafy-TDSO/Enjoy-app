{
  "cli": {
    "appVersionSource": "remote",
    "version": ">= 2.1.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "node": "16.13.0",
      "autoIncrement": true,
      "env": {
        "API_URL": "https://eventos-v1-api.herokuapp.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
